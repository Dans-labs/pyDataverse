@prefix dct: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xs: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix oa: <http://www.openarchives.org/ore/terms/> .
@prefix citation: <https://dataverse.org/schema/citation/> .
@prefix citationdesc: <https://dataverse.org/schema/citation/dsDescription#> .
@prefix citationauthor: <https://dataverse.org/schema/citation/author#> .
@prefix citationcore: <https://dataverse.org/schema/citation/core#> .
@prefix ddialliance: <http://rdf-vocabulary.ddialliance.org/discovery#> .
@prefix odissei: <https://portal.odissei.nl/schema/dansRights#> .
@prefix mlcroissant: <https://mlcommons.org/croissant#> .
@prefix semaf: <http://semaf.com/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

# Semantic artifact mappings
# Name field in Croissant
mlcroissant:name a "name" ;
    rdfs:range xs:string ;
    skos:exactMatch dct:title .

# Description field
mlcroissant:description a "description" ;
    rdfs:range xs:string ;
    skos:exactMatch citation:dsDescriptionValue ;
    skos:exactMatch schema:description ;
    skos:exactMatch citationdesc:Text ;
    skos:exactMatch citation:dsDescriptionText .

# URL field
mlcroissant:url a "url" ;
    rdfs:range xs:string ;
    skos:exactMatch oa:describes .

# "citation": "https://dataverse.org/schema/citation/datasetContactName",
mlcroissant:citation a "citation" ;
    rdfs:range xs:string ;
    skos:exactMatch citation:datasetContactName .

#"keywords": "https://dataverse.org/schema/citation/keywordValue",
mlcroissant:keywords a "keywords" ;
    rdfs:range xs:string ;
    skos:exactMatch citation:keywordValue .

# creators
mlcroissant:creators a "creators" ;
    rdfs:range xs:string ;
    skos:exactMatch citationauthor:Name .

# Version
mlcroissant:version a "version" ;
    rdfs:range xs:string ;
    skos:exactMatch schema:version .
    
# author
mlcroissant:author a "author" ;
    rdfs:range xs:string ;
    skos:exactMatch citationauthor:Name ;
    skos:exactMatch citation:authorName .
#"authoraffiliation": "https://dataverse.org/schema/citation/authorAffiliation"

# In Language
mlcroissant:inLanguage a "in_language" ;
    rdfs:range xs:string ;
    skos:exactMatch dct:language ;
    skos:exactMatch odissei:dansMetadataLanguage .

# License
mlcroissant:license a "license" ;
    rdfs:range xs:string ;
    skos:exactMatch schema:license .

# Publisher
mlcroissant:publisher a "publisher";
    rdfs:range xs:string ;
    skos:exactMatch citation:productionPlace .

# Data Type
mlcroissant:dataType a "data_type";
    rdfs:range xs:string ;
    skos:exactMatch ddialliance:kindOfData .

# Restricted
mlcroissant:restricted a "restricted" ;
    rdfs:range xs:string ;
    skos:exactMatch citationcore:restricted .

# Date Modified
mlcroissant:date_modified a "date_modified" ;
    rdfs:range xs:date ;
    skos:exactMatch schema:dateModified .

# Date Published
mlcroissant:date_published a "date_published" ;
    rdfs:range xs:date ;
    skos:exactMatch schema:datePublished .

# Date Created
mlcroissant:date_created a "date_created" ;
    rdfs:range xs:date ;
    skos:exactMatch dct:dateSubmitted .

